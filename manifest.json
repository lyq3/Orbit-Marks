{
  "manifest_version": 3,
  "name": "OrbitMarks",
  "short_name": "OrbitMarks",
  "version": "1.2",
  "description": "Bookmarks, finally home.",
  "permissions": [
    "bookmarks"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "OrbitMarks"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
